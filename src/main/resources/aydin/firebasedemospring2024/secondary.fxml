<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="aydin.firebasedemospring2024.SecondaryController">
    <children>
        <Label text="Welcome" />
      <TextField fx:id="emailBox" promptText="email" />
      <TextField fx:id="passwordBox" promptText="password" />
        <Button fx:id="secondaryButton" onAction="#registerButtonClicked" text="Register" />
      <Button mnemonicParsing="false" onAction="#signInButtonClicked" text="Sign In" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
